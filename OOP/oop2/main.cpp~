#include <iostream>
#include <string.h>

struct book
{
	char name[31];
	char* author;
};

struct student
{
	unsigned int fn;
	book* studentBook;
};

int main()
{
	book bk;
	std::cin.getline(bk.name, 30);
	strcpy(bk.author, "Author Name");
	student st;
	st.fn = 12345;
	st.studentBook = &bk;
	student* studentList[30];
	studentList[0] = &st;
	std::cout << studentList[0]->studentBook->author << std::endl;

	return 0;
}
